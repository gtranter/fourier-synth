{
  "name": "fourier-synth",
  "version": "1.1.1",
  "description": "Fourier Synthesizer Web Component",
  "keywords": [
    "fourier",
    "stencil",
    "synthesis",
    "synthesizer",
    "web audio",
    "web component"
  ],
  "license": "MIT",
  "author": "G. Tranter",
  "homepage": "https://github.com/gtranter/fourier-synth#readme",
  "bugs": {
    "url": "https://github.com/gtranter/fourier-synth/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gtranter/fourier-synth.git"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/fourier-synth/fourier-synth.esm.js",
  "files": [
    "fourier-synth.html",
    "dist/",
    "loader/",
    "develop.md",
    "LICENSE.txt",
    "package.json",
    "README.md",
    "vscode-docs.json"
  ],
  "scripts": {
    "build": "stencil build",
    "build.deployables": "ts-node ./scripts/create-deployables.ts",
    "build.docs": "stencil build --docs",
    "build.readme": "ts-node ./scripts/update-readme.ts",
    "generate": "stencil generate",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "preversion": "npm run build.docs",
    "version": "npm run build.readme && npm run build.deployables && git add -A",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "ts-node ./scripts/prepend-version.ts"
  },
  "dependencies": {
    "@stencil/core": "^3.0.1"
  },
  "devDependencies": {
    "@stencil/sass": "^2.0.3",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "compressing": "^1.7.0",
    "eslint": "^8.34.0",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "puppeteer": "^19.5.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
