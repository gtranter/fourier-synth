{
  "name": "fourier-synth",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/fourier-synth/fourier-synth.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs && npm run deployable",
    "deployable": "ts-node ./scripts/createDeployable.ts",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "preversion": "stencil build --prod --docs",
    "version": "npm run deployable && git add -A",
    "prepublishOnly": "ts-node ./scripts/prependVersion.ts"
  },
  "dependencies": {
    "@stencil/core": "^3.0.0"
  },
  "devDependencies": {
    "@stencil/sass": "^2.0.3",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "compressing": "^1.7.0",
    "eslint": "^8.34.0",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "puppeteer": "^19.5.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "license": "MIT",
  "author": "G. Tranter"
}
